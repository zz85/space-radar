{
  "name": "space-radar",
  "productName": "SpaceRadar",
  "version": "6.0.0",
  "description": "Disk Usage Electrobun App",
  "main": "app/bun/index.ts",
  "scripts": {
    "format": "prettier app/js/*.js app/css/*.css tools/*.js --single-quote --no-semi --print-width=120 --write",
    "dev": "electrobun dev",
    "build": "electrobun build",
    "start": "bun run build && bun run dev",
    "clean": "rm -rf dist builds",
    "deep_clean": "rm -rf node_modules package-lock.json dist"
  },
  "keywords": [
    "space",
    "memory",
    "disk",
    "usage",
    "visualization",
    "sunburst",
    "treemap",
    "flamecharts",
    "d3"
  ],
  "author": {
    "name": "Joshua Koo",
    "email": "zz85nus@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zz85/space-radar.git"
  },
  "dependencies": {
    "d3": "^3.5.6",
    "d3-ease": "^1.0.3",
    "d3-flame-graph": "4.1.3",
    "d3-hierarchy": "^1.1.5",
    "d3-path": "^0.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^0.5.1",
    "d3-tip": "^0.7.1",
    "electrobun": "^0.8.0",
    "photonkit": "^0.1.2",
    "systeminformation": "^5.30.7",
    "tail-stream": "^0.3.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.9",
    "prettier": "^3.8.1"
  }
}
