{
  "name": "space-radar",
  "productName": "SpaceRadar",
  "version": "7.0.0",
  "description": "Disk Usage Electrobun App",
  "scripts": {
    "dev": "electrobun dev",
    "start": "electrobun dev",
    "build": "electrobun build",
    "build:dev": "electrobun build",
    "build:stable": "electrobun build --env=stable",
    "format": "prettier src/**/*.ts src/**/*.js src/**/*.css --single-quote --no-semi --print-width=120 --write",
    "clean": "rm -rf dist builds",
    "deep_clean": "rm -rf node_modules bun.lock dist"
  },
  "keywords": [
    "space",
    "memory",
    "disk",
    "usage",
    "visualization",
    "sunburst",
    "treemap",
    "flamecharts",
    "d3",
    "electrobun"
  ],
  "author": {
    "name": "Joshua Koo",
    "email": "zz85nus@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zz85/space-radar.git"
  },
  "dependencies": {
    "electrobun": "latest",
    "d3": "^3.5.6",
    "d3-ease": "^1.0.3",
    "d3-flame-graph": "4.1.3",
    "d3-hierarchy": "^1.1.5",
    "d3-path": "^0.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^0.5.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.8.1"
  }
}
